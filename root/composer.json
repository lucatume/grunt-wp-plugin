{
  "name": "{%= author_slug %}/{%= name %}",
  "description": "{%= description %}",
  "type": "wordpress-plugin",
  "license": "GPL 2.0",
  "authors": [
    {
      "name": "{%= author_name %}",
      "email": "{%= author_email %}"
    }
  ],
  "minimum-stability": "stable",
  "config": {
      "preferred-install": "dist"
    },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/lucatume/composer-php52.git"
    }
  ],
  "require": {
    "xrstf/composer-php52": "1.0.20"
  },
    "autoload": {
   "psr-0": {
    "{%= prefix %}_": "src/"
      }
  },
  "scripts": {
    "post-install-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd",
      "grunt after-composer-update"
    ],
    "post-update-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd",
      "grunt after-composer-update"
    ],
    "post-autoload-dump": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ]
  }
}
